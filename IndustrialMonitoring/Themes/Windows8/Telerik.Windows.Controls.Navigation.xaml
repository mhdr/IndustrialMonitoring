    <Style x:Key="LayoutControlSelectionIndicatorStyle" TargetType="layoutControl:LayoutControlSelectionIndicator">
        <Setter Property="Background" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8Resource ResourceKey=AccentBrush}"/>

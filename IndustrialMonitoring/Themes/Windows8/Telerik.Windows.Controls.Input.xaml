    <ControlTemplate x:Key="MaskedInputClearButtonTemplate" TargetType="telerik1:RadButton">
        <Border x:Name="ButtonBorder" Background="Transparent">
            <Path x:Name="Path"
                    Margin="0 -2"
                    Stretch="Fill"
                    Width="11"
                    Height="11"
                    HorizontalAlignment="Center"
                    VerticalAlignment="Center"
                    RenderTransformOrigin="0.5 0.5"
                    Data="M4.0,5.1 L4.9,5.1 L4.9,8.0 L7.8,8.0 L7.8,8.9 L4.9,8.9 L4.9,11.8 L4.0,11.8 L4.0,8.9 L1.1,8.9 L1.1,8.0 L4.0,8.0 z"
                    Stroke="{telerik1:Windows8Resource ResourceKey=BasicBrush}">
                <Path.RenderTransform>
                    <RotateTransform Angle="-45"/>
                </Path.RenderTransform>
            </Path>
        </Border>
    </ControlTemplate>

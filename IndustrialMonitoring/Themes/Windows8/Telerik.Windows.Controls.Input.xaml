                                        Margin="0 0 0 0">
                                    <Path x:Name="Path" Fill="{Binding (TextElement.Foreground), RelativeSource={RelativeSource AncestorType=ContentPresenter}}" Data="M0,0 L6,0 L3,4 z" Margin="5 3"/>

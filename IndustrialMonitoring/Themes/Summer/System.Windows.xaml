                            <Rectangle x:Name="BoxMiddleLine" Stroke="{TemplateBinding BorderBrush}" StrokeThickness="1" Width="10" Height="10" RadiusX="0" RadiusY="0" Opacity=".2" SnapsToDevicePixels="True"/>

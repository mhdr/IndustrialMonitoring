                                    Width="9"
                                    Height="8"
                                    Data="M32.1,77.3 L34.6,80.3 38.8,72.7"
                                    FlowDirection="LeftToRight"
                                    SnapsToDevicePixels="True"/>
                            <Rectangle x:Name="IndeterminateIcon" Height="2" Fill="{StaticResource CheckBox_Path}" Opacity="0" Width="6" SnapsToDevicePixels="True"/>
                            <Rectangle x:Name="DisabledVisualElement" RadiusX="0" RadiusY="0" Width="14" Height="14" Opacity="0" Fill="#FFFFFFFF" SnapsToDevicePixels="True"/>

                                    StrokeThickness="{TemplateBinding BorderThickness}"
                                    Fill="{TemplateBinding Background}"
                                    Margin="1"
                                    SnapsToDevicePixels="True"/>
                            <Rectangle x:Name="BoxMiddle"
                                    Fill="{StaticResource CheckBox_MiddleFill}"

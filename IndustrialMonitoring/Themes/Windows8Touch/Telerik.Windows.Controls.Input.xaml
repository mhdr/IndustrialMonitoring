                                    Margin="0 0 0 1"
                                    ItemTemplate="{TemplateBinding PaletteItemsTemplate}"
                                    ColorPropertyPath="{TemplateBinding ColorPropertyPath}"
                                    VerticalAlignment="Center"
                                    Grid.Row="5"
                                    BorderBrush="Transparent"
                                    BorderThickness="0"

                                                    <Visibility>Visible</Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation Storyboard.TargetName="EditorSite" Storyboard.TargetProperty="Opacity" To="0" Duration="00:00:00"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>

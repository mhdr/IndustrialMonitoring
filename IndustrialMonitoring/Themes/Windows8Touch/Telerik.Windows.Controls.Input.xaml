                </TextBox.Template>
            </TextBox>
            <RepeatButton x:Name="increase" Margin="0 1 1 1" Grid.Column="2" IsTabStop="{TemplateBinding AreButtonsTabStops}" Style="{StaticResource IncreaseRepeatButtonStyle}"/>
            <RepeatButton x:Name="decrease" Margin="1 1 0 1" Grid.Column="0" IsTabStop="{TemplateBinding AreButtonsTabStops}" Style="{StaticResource DecreaseRepeatButtonStyle}"/>
        </Grid>

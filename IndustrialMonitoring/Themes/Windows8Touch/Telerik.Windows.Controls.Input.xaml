                    <telerik:RadButton x:Name="input5_Button"
                            Grid.Row="3"
                            Grid.Column="1"
                            Content="5"
                            FontSize="{TemplateBinding FontSize}"
                            FontWeight="Bold"
                            CommandParameter="5"
                            Command="{x:Static telerikInput:RadCalculatorCommands.UpdateInput}"/>
                    <telerik:RadButton x:Name="input6_Button"
                            Grid.Row="3"
                            Grid.Column="2"
                            Content="6"
                            FontSize="{TemplateBinding FontSize}"
                            FontWeight="Bold"
                            CommandParameter="6"
                            Command="{x:Static telerikInput:RadCalculatorCommands.UpdateInput}"/>
                    <telerik:RadButton x:Name="Multiply_Button"
                            FontSize="{TemplateBinding FontSize}"
                            Grid.Row="3"
                            Grid.Column="3"
                            Content="*"
                            Foreground="{telerik:Windows8TouchResource ResourceKey=HighBrush}"
                            CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Multiply}"

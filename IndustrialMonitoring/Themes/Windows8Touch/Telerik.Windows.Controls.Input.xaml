                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 18 12 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 13 12 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 8 12 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 18 7 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 13 7 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 8 7 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 18 2 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 13 2 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                            <Rectangle Fill="{telerik:Windows8TouchResource ResourceKey=HighBrush}" Margin="0 8 2 0" Height="4" VerticalAlignment="Top" HorizontalAlignment="Right" Width="4"/>
                        </Grid>
                    </Border>
                </Grid>
            </telerik:RadDropDownButton>
        </Grid>

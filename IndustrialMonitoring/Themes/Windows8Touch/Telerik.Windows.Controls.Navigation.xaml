    <Style x:Key="ToolBarToggleButtonStyle" TargetType="ToggleButton">
        <Setter Property="Background" Value="{telerik:Windows8TouchResource ResourceKey=MainGradientBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik:Windows8TouchResource ResourceKey=MediumBrush}"/>

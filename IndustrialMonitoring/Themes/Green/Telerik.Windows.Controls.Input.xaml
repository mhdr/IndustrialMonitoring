    <Style x:Key="RadRatingStyle" TargetType="controls:RadRating">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="HorizontalAlignment" Value="Left"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <StackPanel Orientation="Horizontal"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadRating">
                    <Grid x:Name="Root">
                        <Border x:Name="BorderVisual" Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                            <ItemsPresenter/>
                        </Border>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsFocused" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadRatingItemStyle" TargetType="controls:RadRatingItem">
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Top"/>
        <Setter Property="Padding" Value="2"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:RadRatingItem">
                    <Grid>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="ReadOnly">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetProperty="Fill" Storyboard.TargetName="NormalRatingItem">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <DoubleAnimation Storyboard.TargetProperty="Opacity" Storyboard.TargetName="SelectedRatingItem" To="0.3"/>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FillStates">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition GeneratedDuration="00:00:00.1000000"/>
                                </VisualStateGroup.Transitions>
                                <VisualState x:Name="Empty"/>
                                <VisualState x:Name="Partial"/>
                                <VisualState x:Name="Filled"/>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Grid x:Name="itemGrid" Margin="{TemplateBinding Padding}" Background="Transparent">
                            <Path x:Name="NormalRatingItem"
                                    Fill="{TemplateBinding Background}"
                                    HorizontalAlignment="Left"
                                    Stretch="Fill"
                                    Width="16"
                                    Height="16"
                                    Data="{StaticResource StarIcon}"
                                    Stroke="{TemplateBinding BorderBrush}"
                                    StrokeLineJoin="Round"
                                    VerticalAlignment="Stretch"
                                    StrokeThickness="{TemplateBinding BorderThickness}"/>
                            <telerikRating:LinearClipper RatioVisible="{TemplateBinding RatioVisible}" HorizontalContentAlignment="Center" ExpandDirection="Right">
                                <Path x:Name="MouseOverRatingItem"
                                        Stretch="Fill"
                                        Width="16"
                                        Height="16"
                                        Data="{StaticResource StarIcon}"
                                        StrokeThickness="{TemplateBinding BorderThickness}"
                                        StrokeLineJoin="Round"
                                        Fill="{telerik1:GreenResource ResourceKey=MouseOverBrush}"
                                        Stroke="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            </telerikRating:LinearClipper>
                            <telerikRating:LinearClipper RatioVisible="{TemplateBinding RatioVisible}" HorizontalContentAlignment="Center" ExpandDirection="Right">
                                <Path x:Name="SelectedRatingItem"
                                        Stretch="Fill"
                                        Width="16"
                                        Height="16"
                                        Data="{StaticResource StarIcon}"
                                        StrokeThickness="{TemplateBinding BorderThickness}"
                                        StrokeLineJoin="Round"
                                        Fill="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                                        Stroke="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                            </telerikRating:LinearClipper>
                            <Path x:Name="DisabledRatingItemBackground"
                                    Stretch="Fill"
                                    Width="16"
                                    Height="16"
                                    HorizontalAlignment="Center"
                                    StrokeLineJoin="Round"
                                    VerticalAlignment="Stretch"
                                    Opacity="0"
                                    Data="{StaticResource StarIcon}"
                                    StrokeThickness="{TemplateBinding BorderThickness}"
                                    Fill="{telerik1:GreenResource ResourceKey=AlternativeBrush}"
                                    Stroke="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
                            <telerikRating:LinearClipper x:Name="DisabledRatingItem" HorizontalContentAlignment="Center" RatioVisible="{TemplateBinding RatioVisible}" ExpandDirection="Right" Opacity="0">
                                <Path
                                        Stretch="Fill"
                                        Width="16"
                                        Height="16"
                                        StrokeLineJoin="Round"
                                        VerticalAlignment="Stretch"
                                        Data="{StaticResource StarIcon}"
                                        Fill="{telerik1:GreenResource ResourceKey=BasicBrush}"
                                        Stroke="{x:Null}"
                                        StrokeThickness="{TemplateBinding BorderThickness}"/>
                            </telerikRating:LinearClipper>
                            <telerikRating:LinearClipper RatioVisible="{TemplateBinding ValueRatio}" ExpandDirection="Right" HorizontalContentAlignment="Center">
                                <Path x:Name="SelectedUnHoveredRatingItem"
                                        Stretch="Fill"
                                        Width="16"
                                        Height="16"
                                        StrokeThickness="{TemplateBinding BorderThickness}"
                                        StrokeLineJoin="Round"
                                        VerticalAlignment="Stretch"
                                        Opacity="0"
                                        Data="{StaticResource StarIcon}"
                                        Fill="{x:Null}"
                                        Stroke="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            </telerikRating:LinearClipper>
                        </Grid>
                        <ContentPresenter x:Name="Content"
                                Cursor="{TemplateBinding Cursor}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                ContentTemplate="{TemplateBinding ContentTemplate}"
                                Grid.Row="1"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOverItem" Value="True">
                            <Setter Property="Opacity" TargetName="MouseOverRatingItem" Value="1"/>
                            <Setter Property="Opacity" TargetName="SelectedRatingItem" Value="0"/>
                            <Setter Property="Opacity" TargetName="SelectedUnHoveredRatingItem" Value="0"/>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Trigger.EnterActions>
                                <BeginStoryboard x:Name="SelectedState">
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="0" Storyboard.TargetName="MouseOverRatingItem" Storyboard.TargetProperty="(UIElement.Opacity)"/>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetName="SelectedRatingItem" Storyboard.TargetProperty="(UIElement.Opacity)"/>
                                        <DoubleAnimation Duration="0" To="0" Storyboard.TargetName="SelectedUnHoveredRatingItem" Storyboard.TargetProperty="(UIElement.Opacity)"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.EnterActions>
                            <Trigger.ExitActions>
                                <BeginStoryboard>
                                    <Storyboard>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetName="MouseOverRatingItem" Storyboard.TargetProperty="(UIElement.Opacity)"/>
                                        <DoubleAnimation Duration="0" To="0" Storyboard.TargetName="SelectedRatingItem" Storyboard.TargetProperty="(UIElement.Opacity)"/>
                                        <DoubleAnimation Duration="0" To="1" Storyboard.TargetName="SelectedUnHoveredRatingItem" Storyboard.TargetProperty="(UIElement.Opacity)"/>
                                    </Storyboard>
                                </BeginStoryboard>
                            </Trigger.ExitActions>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Opacity" TargetName="DisabledRatingItem" Value="1"/>
                            <Setter Property="Opacity" TargetName="DisabledRatingItemBackground" Value="1"/>
                            <Setter Property="Opacity" TargetName="NormalRatingItem" Value="0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="controls:RadRating" BasedOn="{StaticResource RadRatingStyle}"/>
    <Style TargetType="controls:RadRatingItem" BasedOn="{StaticResource RadRatingItemStyle}"/>
    <Style x:Key="TimeSpanPickerWatermarkTextBoxStyle" TargetType="telerik:RadWatermarkTextBox">

    <Style x:Key="CalculatorButtonStyle" TargetType="telerik:RadButton" BasedOn="{StaticResource RadButtonStyle}">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="MinHeight" Value="28"/>
        <Setter Property="MinWidth" Value="36"/>
        <Setter Property="Margin" Value="2"/>
        <Setter Property="Focusable" Value="False"/>
    </Style>
    <ControlTemplate x:Key="RadCalculatorTemplate" TargetType="controls:RadCalculator">
        <Grid Background="Transparent" SnapsToDevicePixels="True">
            <Border
                    Background="{TemplateBinding Background}"
                    BorderBrush="{TemplateBinding BorderBrush}"
                    BorderThickness="{TemplateBinding BorderThickness}"
                    CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                    SnapsToDevicePixels="True">
                <Grid x:Name="Root" Margin="{TemplateBinding Padding}">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="Auto"/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Border
                            MinHeight="58"
                            BorderBrush="{telerik1:GreenResource ResourceKey=AccentLowBrush}"
                            BorderThickness="1"
                            Margin="2"
                            Padding="4"
                            CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                            SnapsToDevicePixels="True">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="Auto"/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="auto"/>
                                <ColumnDefinition Width="*"/>
                            </Grid.ColumnDefinitions>
                            <TextBox x:Name="DisplayTextBox"
                                    Grid.Row="1"
                                    Grid.ColumnSpan="2"
                                    Text="{Binding DisplayedValue, RelativeSource={RelativeSource TemplatedParent}}"
                                    Style="{StaticResource CalculatorDisplayValue_Style}"
                                    Foreground="{TemplateBinding Foreground}"/>
                            <TextBlock x:Name="MemoryIndicatorTextBox"
                                    Text="M"
                                    Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsMemoryUpdated, Converter={StaticResource BooleanToVisibilityConverter}}"
                                    VerticalAlignment="Top"
                                    FontSize="{telerik1:GreenResource ResourceKey=FontSize}"/>
                            <TextBox x:Name="HistoryTextBox"
                                    Grid.Column="1"
                                    Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=History, Mode=OneWay}"
                                    Style="{StaticResource CalculatorHistoryDisplay_Style}"
                                    Foreground="{TemplateBinding Foreground}"/>
                        </Grid>
                    </Border>
                    <Grid x:Name="btnContainer" Grid.Row="1">
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="row"/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                            <RowDefinition/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                            <ColumnDefinition/>
                        </Grid.ColumnDefinitions>
                        <telerik:RadButton x:Name="Mc_Button" Content="MC" Grid.Column="0" Command="{x:Static controls:RadCalculatorCommands.MemoryClear}" BorderThickness="0" Background="Transparent"/>
                        <telerik:RadButton x:Name="Mr_Button" Content="MR" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.MemoryRecall}" BorderThickness="0" Background="Transparent"/>
                        <telerik:RadButton x:Name="Ms_Button" Content="MS" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.MemoryStore}" BorderThickness="0" Background="Transparent"/>
                        <telerik:RadButton x:Name="Mplus_Button" Content="M+" Grid.Column="3" Command="{x:Static controls:RadCalculatorCommands.MemoryPlus}" BorderThickness="0" Background="Transparent"/>
                        <telerik:RadButton x:Name="Mminus_Button" Content="M-" Grid.Column="4" Command="{x:Static controls:RadCalculatorCommands.MemoryMinus}" BorderThickness="0" Background="Transparent"/>
                        <telerik:RadButton x:Name="Delete_Button"
                                Content=""
                                Grid.Row="1"
                                Grid.Column="0"
                                Command="{x:Static controls:RadCalculatorCommands.Delete}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="ClearInput_Button"
                                Content="CE"
                                Grid.Row="1"
                                Grid.Column="1"
                                Command="{x:Static controls:RadCalculatorCommands.ClearInput}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="ClearEverything_Button"
                                Content="C"
                                Grid.Row="1"
                                Grid.Column="2"
                                Command="{x:Static controls:RadCalculatorCommands.ClearEverything}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="Negate_Button"
                                Content=""
                                Grid.Row="1"
                                Grid.Column="3"
                                Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Negate}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="SquareRoot_Button"
                                Content=""
                                Grid.Row="1"
                                Grid.Column="4"
                                Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.SquareRoot}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="input7_Button" Content="7" Grid.Row="2" Grid.Column="0" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="7" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="input8_Button" Content="8" Grid.Row="2" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="8" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="input9_Button" Content="9" Grid.Row="2" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="9" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="Divide_Button"

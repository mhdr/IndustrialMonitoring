                            <Setter Property="Visibility" TargetName="HlsTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="HsvTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="CmykTemplate" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="ColorMode" Value="HSV">
                            <Setter Property="Visibility" TargetName="RgbTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="HlsTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="HsvTemplate" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="CmykTemplate" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="ColorMode" Value="HLS">
                            <Setter Property="Visibility" TargetName="RgbTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="HlsTemplate" Value="Visible"/>
                            <Setter Property="Visibility" TargetName="HsvTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="CmykTemplate" Value="Collapsed"/>
                        </Trigger>
                        <Trigger Property="ColorMode" Value="CMYK">
                            <Setter Property="Visibility" TargetName="RgbTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="HlsTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="HsvTemplate" Value="Collapsed"/>
                            <Setter Property="Visibility" TargetName="CmykTemplate" Value="Visible"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

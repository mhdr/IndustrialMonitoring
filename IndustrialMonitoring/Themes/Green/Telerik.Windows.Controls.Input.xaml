                                Grid.Column="4"
                                Command="{x:Static controls:RadCalculatorCommands.ExecutePercentOperation}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="input4_Button" Content="4" Grid.Row="3" Grid.Column="0" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="4" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="input5_Button" Content="5" Grid.Row="3" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="5" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="input6_Button" Content="6" Grid.Row="3" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="6" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="Multiply_Button"
                                Content="*"
                                Grid.Row="3"
                                Grid.Column="3"
                                Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Multiply}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="Reciprocal_Button"
                                Content="1/x"
                                Grid.Row="3"
                                Grid.Column="4"
                                Command="{x:Static controls:RadCalculatorCommands.ExecuteUnaryOperation}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Reciprocal}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="input1_Button" Content="1" Grid.Row="4" Grid.Column="0" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="1" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="input2_Button" Content="2" Grid.Row="4" Grid.Column="1" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="2" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="input3_Button" Content="3" Grid.Row="4" Grid.Column="2" Command="{x:Static controls:RadCalculatorCommands.UpdateInput}" CommandParameter="3" FontWeight="Bold"/>
                        <telerik:RadButton x:Name="Substract_Button"
                                Content="-"
                                Grid.Row="4"
                                Grid.Column="3"
                                Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Substract}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="input0_Button"
                                Content="0"
                                Grid.Row="5"
                                Grid.Column="0"
                                Grid.ColumnSpan="2"
                                Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                CommandParameter="0"
                                FontWeight="Bold"/>
                        <telerik:RadButton x:Name="FloatingPoint_Button"
                                Content="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=DecimalSeparator}"
                                Grid.Row="5"
                                Grid.Column="2"
                                Command="{x:Static controls:RadCalculatorCommands.UpdateInput}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=DecimalSeparator}"
                                FontWeight="Bold"/>
                        <telerik:RadButton x:Name="Add_Button"
                                Content="+"
                                Grid.Row="5"
                                Grid.Column="3"
                                Command="{x:Static controls:RadCalculatorCommands.AddPendingBinaryOperation}"
                                CommandParameter="{Binding RelativeSource={RelativeSource Mode=TemplatedParent}, Path=Operations.Add}"
                                BorderThickness="0"
                                Background="Transparent"/>
                        <telerik:RadButton x:Name="Equals_Button" Content="=" Grid.Row="4" Grid.Column="4" Grid.RowSpan="2" Command="{x:Static controls:RadCalculatorCommands.ExecuteBinaryOperation}"/>
                    </Grid>
                </Grid>
            </Border>
        </Grid>
        <ControlTemplate.Triggers>
            <Trigger Property="MemoryButtonsVisibility" Value="Collapsed">
                <Setter Property="Visibility" TargetName="Mc_Button" Value="Collapsed"/>
                <Setter Property="Visibility" TargetName="Mr_Button" Value="Collapsed"/>
                <Setter Property="Visibility" TargetName="Ms_Button" Value="Collapsed"/>
                <Setter Property="Visibility" TargetName="Mplus_Button" Value="Collapsed"/>
                <Setter Property="Visibility" TargetName="Mminus_Button" Value="Collapsed"/>
                <Setter Property="Height" TargetName="row" Value="0"/>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="RadCalculatorStyle" TargetType="controls:RadCalculator">
        <Setter Property="Template" Value="{StaticResource RadCalculatorTemplate}"/>
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AlternativeBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="2"/>
        <Setter Property="HorizontalAlignment" Value="Center"/>
        <Setter Property="VerticalAlignment" Value="Center"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Style.Resources>
            <Style TargetType="telerik:RadButton" BasedOn="{StaticResource CalculatorButtonStyle}"/>
        </Style.Resources>
    </Style>
    <Style TargetType="controls:RadCalculator" BasedOn="{StaticResource RadCalculatorStyle}"/>
    <Style x:Key="RadWatermarkTextBoxStyle" TargetType="telerik1:RadWatermarkTextBox">
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
        <Setter Property="AllowDrop" Value="True"/>
        <Setter Property="IsTabStop" Value="True"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="3 2"/>
        <Setter Property="KeyboardNavigation.TabNavigation" Value="Once"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="telerik1:CaretBrushHelper.CaretBrush" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=MainBrush}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="Validation.ErrorTemplate" Value="{StaticResource ValidationTooltipTemplate}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="SelectionBrush" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
        <Setter Property="Template">
            <Setter.Value>

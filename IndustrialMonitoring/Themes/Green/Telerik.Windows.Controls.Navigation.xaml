            <Trigger Property="Role" Value="SubmenuHeader">
                <Setter Property="Template" Value="{Binding Path=SubmenuHeaderTemplateKey, RelativeSource={RelativeSource Self}}"/>
            </Trigger>
            <Trigger Property="Role" Value="SubmenuItem">
                <Setter Property="Template" Value="{Binding Path=SubmenuItemTemplateKey, RelativeSource={RelativeSource Self}}"/>
            </Trigger>
            <Trigger Property="Role" Value="Separator">
                <Setter Property="Template" Value="{Binding Path=SeparatorTemplateKey, RelativeSource={RelativeSource Self}}"/>

                    Orientation="Horizontal"
                    VerticalAlignment="Top"
                    HorizontalAlignment="Right"
                    DataContext="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=telerikNavigation:RadDesktopAlert}}">
                <telerik:RadDropDownButton x:Name="DesktopAlert_DropDownButton"
                        IsOpen="{Binding IsMenuOpen, Mode=TwoWay}"
                        Style="{StaticResource DesktopAlertDropDownButtonStyle}"
                        Visibility="{Binding ShowMenuButton, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <telerik:RadDropDownButton.DropDownContent>
                        <telerikNavigation:RadContextMenu ItemsSource="{Binding MenuItemsSource}" ItemContainerStyle="{Binding MenuItemContainerStyle}"/>
                    </telerik:RadDropDownButton.DropDownContent>
                </telerik:RadDropDownButton>
                <telerik:RadButton x:Name="DesktopAlert_CloseButton"
                        Command="{x:Static telerik:DesktopAlertCommands.Close}"
                        Style="{StaticResource DesktopAlertCloseButtonStyle}"
                        Visibility="{Binding ShowCloseButton, Converter={StaticResource BooleanToVisibilityConverter}}">
                    <ToolTipService.ToolTip>
                        <ToolTip telerik:LocalizationManager.ResourceKey="Close"/>
                    </ToolTipService.ToolTip>
                </telerik:RadButton>
            </StackPanel>
            <ContentPresenter
                    Content="{Binding}"
                    Grid.RowSpan="2"
                    VerticalAlignment="Top"
                    TextBlock.FontWeight="Bold"
                    MinHeight="{Binding ActualHeight, ElementName=DesktopAlert_CloseButton}"
                    Margin="0 2 42 0">
                <ContentPresenter.Resources>
                    <Style TargetType="TextBlock">
                        <Setter Property="TextWrapping" Value="Wrap"/>
                    </Style>
                </ContentPresenter.Resources>
            </ContentPresenter>
        </Grid>
    </DataTemplate>
    <Storyboard x:Key="DesktopAlertOnMouseEnter">
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="Opacity" To="1"/>
    </Storyboard>
    <Storyboard x:Key="DesktopAlertOnMouseLeave">
        <DoubleAnimation Duration="0:0:0.5" Storyboard.TargetProperty="Opacity" To="0.9"/>
    </Storyboard>
    <ControlTemplate x:Key="RadDesktopAlertTemplate" TargetType="telerikNavigation:RadDesktopAlert">
        <Border
                Background="{TemplateBinding Background}"
                BorderBrush="{TemplateBinding BorderBrush}"
                BorderThickness="{TemplateBinding BorderThickness}"
                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"
                SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
            <Grid Margin="{TemplateBinding Padding}">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <ContentPresenter
                        Content="{TemplateBinding Header}"
                        ContentTemplate="{TemplateBinding HeaderTemplate}"
                        Grid.Row="0"
                        Grid.Column="1"
                        VerticalAlignment="Top"
                        TextBlock.Foreground="{TemplateBinding Foreground}"/>
                <ContentPresenter x:Name="Icon"
                        Grid.Column="0"
                        Grid.RowSpan="2"
                        Content="{TemplateBinding Icon}"
                        Width="{TemplateBinding IconColumnWidth}"
                        Margin="{TemplateBinding IconMargin}"
                        VerticalAlignment="Center"
                        HorizontalAlignment="Center"
                        ContentTemplate="{TemplateBinding IconTemplate}"/>
                <ContentPresenter x:Name="Content"
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="0 7 0 0"
                        TextBlock.Foreground="{TemplateBinding Foreground}"
                        telerikNavigation:WindowHost.HitTestable="True"
                        VerticalAlignment="Top"
                        HorizontalAlignment="Left"
                        Cursor="Hand">
                    <ContentPresenter.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="TextWrapping" Value="Wrap"/>
                            <Setter Property="TextTrimming" Value="CharacterEllipsis"/>
                            <Style.Triggers>
                                <Trigger Property="TextBlock.IsMouseOver" Value="True">
                                    <Setter Property="TextDecorations" Value="Underline"/>
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </ContentPresenter.Resources>
                </ContentPresenter>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOverAlert" Value="True">
                <Trigger.EnterActions>
                    <BeginStoryboard Storyboard="{StaticResource DesktopAlertOnMouseEnter}"/>
                </Trigger.EnterActions>
                <Trigger.ExitActions>
                    <BeginStoryboard Storyboard="{StaticResource DesktopAlertOnMouseLeave}"/>
                </Trigger.ExitActions>
            </Trigger>
        </ControlTemplate.Triggers>
    </ControlTemplate>
    <Style x:Key="DesktopAlertMenuItemContainerStyle" TargetType="telerikNavigation:RadMenuItem" BasedOn="{StaticResource RadMenuItemStyle}">
        <Setter Property="Header" Value="{Binding Header}"/>
        <Setter Property="Command" Value="{Binding Command}"/>
        <Setter Property="CommandParameter" Value="{Binding CommandParameter}"/>
        <Setter Property="ItemsSource" Value="{Binding ItemsSource}"/>
        <Setter Property="IsSeparator" Value="{Binding IsSeparator}"/>
        <Setter Property="IsCheckable" Value="{Binding IsCheckable}"/>
        <Setter Property="IsChecked" Value="{Binding IsChecked}"/>
        <Setter Property="Icon" Value="{Binding IconUrl}"/>
        <Setter Property="IconTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Image Source="{Binding}" Stretch="None"/>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="RadDesktopAlertStyle" TargetType="telerikNavigation:RadDesktopAlert">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Foreground" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
        <Setter Property="Padding" Value="9 4 4 8"/>
        <Setter Property="Opacity" Value="0.9"/>
        <Setter Property="Width" Value="360"/>
        <Setter Property="Height" Value="100"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="HeaderTemplate" Value="{StaticResource RadDesktopAlertHeaderTemplate}"/>
        <Setter Property="MenuItemContainerStyle" Value="{StaticResource DesktopAlertMenuItemContainerStyle}"/>
        <Setter Property="Template" Value="{StaticResource RadDesktopAlertTemplate}"/>
    </Style>
    <Style TargetType="telerikNavigation:RadDesktopAlert" BasedOn="{StaticResource RadDesktopAlertStyle}"/>
</ResourceDictionary>

    <Style x:Key="RadToolBarStyle" TargetType="telerikNavigation:RadToolBar">
        <Setter Property="FontFamily" Value="{telerik1:GreenResource ResourceKey=FontFamily}"/>
        <Setter Property="FontSize" Value="{telerik1:GreenResource ResourceKey=FontSize}"/>
        <Setter Property="Background" Value="{telerik1:GreenResource ResourceKey=PrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=BasicBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="MinWidth" Value="26"/>
        <Setter Property="Margin" Value="1"/>
        <Setter Property="Padding" Value="3"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="FocusManager.IsFocusScope" Value="True"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="ItemContainerStyleSelector">
            <Setter.Value>
                <telerikNavigation:ToolBarContainerStyleSelector>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadToolBarSeparator" ContainerStyle="{StaticResource RadToolBarSeparatorStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="TextBlock" ContainerStyle="{StaticResource ToolBarTextBlockStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="TextBox" ContainerStyle="{StaticResource ToolBarTextBoxStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="Button" ContainerStyle="{StaticResource ToolBarButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="ToggleButton" ContainerStyle="{StaticResource ToolBarToggleButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="CheckBox" ContainerStyle="{StaticResource ToolBarCheckBoxStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadioButton" ContainerStyle="{StaticResource ToolBarRadioButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadButton" ContainerStyle="{StaticResource ToolBarRadButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadPathButton" ContainerStyle="{StaticResource ToolBarRadPathButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadToggleButton" ContainerStyle="{StaticResource ToolBarRadToggleButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadRadioButton" ContainerStyle="{StaticResource ToolBarRadRadioButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadDropDownButton" ContainerStyle="{StaticResource ToolBarRadDropDownButtonStyle}"/>
                    <telerikNavigation:ToolBarContainerStyle TypeName="RadSplitButton" ContainerStyle="{StaticResource ToolBarRadSplitButtonStyle}"/>
                </telerikNavigation:ToolBarContainerStyleSelector>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadToolBar">
                    <Grid x:Name="RootElement" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition x:Name="c0" Width="Auto"/>
                            <ColumnDefinition x:Name="c1" Width="*"/>
                            <ColumnDefinition x:Name="c2" Width="Auto"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition x:Name="r0" Height="Auto"/>
                            <RowDefinition x:Name="r1" Height="*"/>
                            <RowDefinition x:Name="r2" Height="Auto"/>
                        </Grid.RowDefinitions>
                        <Border x:Name="BorderVisual"
                                Grid.ColumnSpan="3"
                                Grid.RowSpan="3"
                                Background="{TemplateBinding Background}"
                                BorderBrush="{TemplateBinding BorderBrush}"
                                BorderThickness="{TemplateBinding BorderThickness}"
                                CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}"/>
                        <telerik:RadToggleButton x:Name="PART_DropdownButton"
                                Grid.Column="2"
                                Grid.RowSpan="2"
                                Visibility="{TemplateBinding OverflowButtonVisibility}"
                                IsEnabled="{TemplateBinding HasOverflowItems}"
                                IsChecked="{TemplateBinding IsOverflowOpen}"
                                Style="{StaticResource ToolBarOverflowButtonStyle}">
                            <Path x:Name="arrow"
                                    Data="{StaticResource DropDownArrow}"
                                    Fill="{Binding (TextElement.Foreground), RelativeSource={RelativeSource AncestorType=ContentPresenter}}"
                                    Stretch="None"/>
                        </telerik:RadToggleButton>
                        <Grid x:Name="PopupGrid" Grid.Column="2">
                            <Popup x:Name="PART_Overflow"
                                    AllowsTransparency="True"
                                    StaysOpen="False"
                                    Focusable="false"
                                    Placement="Bottom"
                                    VerticalOffset="1"
                                    HorizontalOffset="0"
                                    IsOpen="{Binding Path=IsOverflowOpen, RelativeSource={RelativeSource TemplatedParent}}"
                                    PopupAnimation="{DynamicResource {x:Static SystemParameters.ComboBoxPopupAnimationKey}}">
                                <Border
                                        BorderBrush="{telerik1:GreenResource ResourceKey=BasicBrush}"
                                        Background="{telerik1:GreenResource ResourceKey=PrimaryBrush}"
                                        BorderThickness="1"
                                        MinWidth="55"
                                        MinHeight="26"
                                        CornerRadius="{telerik1:GreenResource ResourceKey=CornerRadius}">
                                    <telerikToolBar:RadToolBarOverflowPanel x:Name="PART_OverflowPanel" Orientation="Vertical" VerticalAlignment="Top"/>
                                </Border>
                            </Popup>
                        </Grid>
                        <Grid Visibility="{TemplateBinding GripVisibility}" Background="Transparent">
                            <Path x:Name="GripOrnamentH"
                                    Visibility="Visible"
                                    Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                    VerticalAlignment="Center"
                                    HorizontalAlignment="Left"
                                    Data="{StaticResource HorizontalGripIcon}"
                                    Margin="6 0 8 0"/>
                            <Path x:Name="GripOrnamentV"
                                    Visibility="Collapsed"
                                    Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                                    VerticalAlignment="Top"
                                    HorizontalAlignment="Center"
                                    Data="{StaticResource VerticalGripIcon}"
                                    Margin="0 6 0 8"/>
                        </Grid>
                        <telerikToolBar:RadToolBarPanel x:Name="PART_StripPanel" Grid.Column="1" Orientation="Horizontal" Margin="{TemplateBinding Padding}"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="Orientation" Value="Horizontal"/>
                        <Trigger Property="Orientation" Value="Vertical">
                            <Setter TargetName="c0" Property="Width" Value="*"/>
                            <Setter TargetName="c1" Property="Width" Value="Auto"/>
                            <Setter TargetName="c2" Property="Width" Value="Auto"/>
                            <Setter TargetName="PART_StripPanel" Property="Orientation" Value="Vertical"/>
                            <Setter TargetName="PART_StripPanel" Property="Grid.Row" Value="1"/>
                            <Setter TargetName="PART_StripPanel" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Row" Value="2"/>
                            <Setter TargetName="PART_DropdownButton" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_DropdownButton" Property="LayoutTransform">
                                <Setter.Value>
                                    <TransformGroup>
                                        <RotateTransform Angle="-90" CenterX="0" CenterY="0"/>
                                    </TransformGroup>
                                </Setter.Value>
                            </Setter>
                            <Setter TargetName="GripOrnamentH" Property="Visibility" Value="Collapsed"/>
                            <Setter TargetName="GripOrnamentV" Property="Visibility" Value="Visible"/>
                            <Setter TargetName="PopupGrid" Property="Grid.Row" Value="2"/>
                            <Setter TargetName="PopupGrid" Property="Grid.Column" Value="0"/>
                            <Setter TargetName="PART_Overflow" Property="Placement" Value="Right"/>
                            <Setter TargetName="PART_Overflow" Property="HorizontalOffset" Value="1"/>
                            <Setter TargetName="PART_Overflow" Property="VerticalOffset" Value="0"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

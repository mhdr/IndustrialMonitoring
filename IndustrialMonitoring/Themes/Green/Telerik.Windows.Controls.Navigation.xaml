                <Grid x:Name="PageRight" Width="9" Height="9" Opacity="0" Background="Transparent">
                    <Path x:Name="arrowPageRight"
                            Data="{StaticResource ArrowPageRight}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            SnapsToDevicePixels="True"
                            HorizontalAlignment="Center"
                            Stretch="Fill"
                            Width="9"
                            Height="9"
                            VerticalAlignment="Center"/>
                </Grid>
                <Grid x:Name="PageUp" Width="9" Height="9" Opacity="0" Background="Transparent">
                    <Path x:Name="arrowPageUp"
                            Data="{StaticResource ArrowPageUp}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            SnapsToDevicePixels="True"
                            HorizontalAlignment="Center"
                            Stretch="Fill"
                            Width="9"
                            Height="9"
                            VerticalAlignment="Center"/>
                </Grid>
                <Grid x:Name="LineUp" Width="9" Height="9" Opacity="0" Background="Transparent">
                    <Path x:Name="arrowLineUp"
                            Data="{StaticResource ArrowUp9x7}"
                            SnapsToDevicePixels="True"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            HorizontalAlignment="Center"
                            Stretch="Fill"
                            Width="9"
                            Height="7"
                            VerticalAlignment="Center"/>
                </Grid>
                <Grid x:Name="LineDown" Width="9" Height="9" Opacity="0" Background="Transparent">
                    <Path x:Name="arrowLineDown"
                            Data="{StaticResource ArrowDown9x7}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            SnapsToDevicePixels="True"
                            HorizontalAlignment="Center"
                            Stretch="Fill"
                            Width="9"
                            Height="7"
                            VerticalAlignment="Center"/>
                </Grid>
                <Grid x:Name="PageDown" Width="9" Height="9" Opacity="0" Background="Transparent">
                    <Path x:Name="arrowPageDown"
                            Data="{StaticResource ArrowPageDown}"
                            Fill="{telerik1:GreenResource ResourceKey=StrongBrush}"
                            SnapsToDevicePixels="True"
                            HorizontalAlignment="Center"
                            Stretch="Fill"
                            Width="9"
                            Height="9"
                            VerticalAlignment="Center"/>
                </Grid>
            </Grid>
        </Border>
        <ControlTemplate.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" TargetName="Arrow" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter Property="BorderBrush" TargetName="Arrow" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                <Setter Property="BorderThickness" TargetName="Arrow" Value="1"/>
                <Setter Property="Fill" TargetName="arrowPageLeft" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineLeft" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineRight" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowPageRight" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowPageUp" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineUp" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineDown" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowPageDown" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
                <Setter Property="Background" TargetName="Arrow" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="BorderBrush" TargetName="Arrow" Value="{telerik1:GreenResource ResourceKey=AccentHighBrush}"/>
                <Setter Property="BorderThickness" TargetName="Arrow" Value="1"/>
                <Setter Property="Fill" TargetName="arrowPageLeft" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineLeft" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineRight" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowPageRight" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowPageUp" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineUp" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowLineDown" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                <Setter Property="Fill" TargetName="arrowPageDown" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="BorderBrush" TargetName="Arrow" Value="{telerik1:GreenResource ResourceKey=AccentLowBrush}"/>
                <Setter Property="BorderThickness" TargetName="Arrow" Value="1"/>

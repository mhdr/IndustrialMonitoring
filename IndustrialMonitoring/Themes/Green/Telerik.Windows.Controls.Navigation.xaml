                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="ForegroundIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{telerik1:GreenResource ResourceKey=MouseOverBrush}"/>
                            <Setter TargetName="ForegroundIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=SelectedBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="ForegroundIcon" Property="Opacity" Value="{telerik1:GreenResource ResourceKey=DisabledOpacity}"/>
                            <Setter TargetName="ForegroundIcon" Property="Fill" Value="{telerik1:GreenResource ResourceKey=MarkerBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="MenuScrollDownButton" TargetType="RepeatButton" BasedOn="{StaticResource MenuScrollButton}">
        <Setter Property="BorderThickness" Value="0 1 0 0"/>
        <Setter Property="Template">

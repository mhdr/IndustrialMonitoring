                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="Background" Value="{StaticResource TabItem_Background_MouseOver}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{StaticResource TabItem_OuterBorder_MouseOver}"/>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="BorderVisual" Property="Margin" Value="{StaticResource TabItem_Margin_Selected}"/>
                            <Setter TargetName="InnerBorderVisual" Property="BorderBrush" Value="{StaticResource TabItem_InnerBorder_Selected}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{StaticResource TabItem_OuterBorder_Selected}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{StaticResource TabItem_Background_Selected}"/>
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="True"/>
                                <Condition Property="IsMouseOver" Value="True"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="BorderVisual" Property="Margin" Value="{StaticResource TabItem_Margin_Selected}"/>
                            <Setter TargetName="InnerBorderVisual" Property="BorderBrush" Value="{StaticResource TabItem_InnerBorder_Selected}"/>
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{StaticResource TabItem_OuterBorder_SelectedMouseOver}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{StaticResource TabItem_Background_Selected}"/>
                        </MultiTrigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="wrapper" Property="Opacity" Value="0.3"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

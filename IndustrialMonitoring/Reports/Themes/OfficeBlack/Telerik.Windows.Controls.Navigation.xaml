    <ControlTemplate x:Key="ToolBarDropdownButton" TargetType="telerik:RadToggleButton">
        <Grid Background="Transparent">
            <VisualStateManager.VisualStateGroups>
                <VisualStateGroup x:Name="CommonStates">
                    <VisualState x:Name="Normal"/>
                    <VisualState x:Name="Disabled">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="OrnamentGrid" Storyboard.TargetProperty="Opacity" To="0.25"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="MouseOver">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="MouseOverDecorator" Storyboard.TargetProperty="Opacity" To="1"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Pressed">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="backgroundDecoratorCheck" Storyboard.TargetProperty="Opacity" To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
                <VisualStateGroup x:Name="CheckStates">
                    <VisualState x:Name="Unchecked"/>
                    <VisualState x:Name="Checked">
                        <Storyboard>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="backgroundDecoratorCheck" Storyboard.TargetProperty="Opacity" To="1"/>
                        </Storyboard>
                    </VisualState>
                </VisualStateGroup>
            </VisualStateManager.VisualStateGroups>
            <Border
                    BorderThickness="0 1 1 1"
                    BorderBrush="{StaticResource ToolBar_ToggleButton_normalOuterBorder}"
                    Background="{StaticResource ToolBar_ToggleButton_normalBackground}"
                    x:Name="backgroundDecorator"
                    CornerRadius="0 3 3 0">
                <Border BorderThickness="{StaticResource ToolBar_ToggleButton_InnerBorderThickness}" CornerRadius="0 2 2 0" BorderBrush="{StaticResource ToolBar_ToggleButton_normalInnerBorder}"/>
            </Border>
            <Border
                    BorderThickness="0 1 1 1"
                    BorderBrush="{StaticResource ControlOuterBorder_MouseOver}"
                    Background="{StaticResource ControlBackground_MouseOver}"
                    x:Name="MouseOverDecorator"
                    CornerRadius="0 3 3 0"
                    Opacity="0">
                <Border BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="0 2 2 0" BorderBrush="{StaticResource ControlInnerBorder_MouseOver}"/>
            </Border>
            <Border
                    BorderThickness="1"
                    BorderBrush="{StaticResource ControlOuterBorder_Pressed}"
                    Background="{StaticResource ControlBackground_Pressed}"
                    x:Name="backgroundDecoratorCheck"
                    CornerRadius="0 3 3 0"
                    Opacity="0">
                <Border BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="0 2 2 0" BorderBrush="{StaticResource ControlInnerBorder_Pressed}"/>
            </Border>
            <Grid x:Name="OrnamentGrid" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0 0 3 3" Opacity="1">
                <Path
                        Fill="#FFFFFFFF"
                        Stretch="Fill"
                        Margin="2,5,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="5"
                        Height="3"
                        Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z"/>
                <Path
                        Fill="{StaticResource ControlElement_Normal}"
                        Stretch="Fill"
                        Margin="0,4,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="5"
                        Height="3"
                        Data="M0,0 L5,0 5,1 4,1 4,2 3,2 3,3 2,3 2,2 1,2 1,1 0,1 0,0 z"/>
                <Path Fill="#FFFFFFFF" Stretch="Fill" Margin="2,1,0,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="5" Height="1" Data="M0,0 L5,0 5,1 0,1 0,0 z"/>
                <Path
                        Fill="{StaticResource ControlElement_Normal}"
                        Stretch="Fill"
                        Margin="0,0,0,0"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        Width="5"
                        Height="1"
                        Data="M0,0 L5,0 5,1 0,1 0,0 z"/>
            </Grid>
        </Grid>
    </ControlTemplate>
    <Style TargetType="telerikNavigation:RadToolBar" x:Key="RadToolBarStyle">

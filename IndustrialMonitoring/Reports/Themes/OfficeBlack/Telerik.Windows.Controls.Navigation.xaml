        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="TopLevelHeaderTemplateKey" Value="{StaticResource OutlookBarMenuItemTemplate}"/>
        <Setter Property="TopLevelItemTemplateKey" Value="{StaticResource OutlookBarMenuItemTemplate}"/>
        <Setter Property="SubmenuHeaderTemplateKey" Value="{StaticResource OutlookBarMenuItemTemplate}"/>
        <Setter Property="SubmenuItemTemplateKey" Value="{StaticResource OutlookBarMenuItemTemplate}"/>

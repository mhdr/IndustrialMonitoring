                                    <Storyboard>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="GripOrnamentH" Storyboard.TargetProperty="Opacity" To="0.5"/>
                                        <DoubleAnimation Duration="0" Storyboard.TargetName="GripOrnamentV" Storyboard.TargetProperty="Opacity" To="0.5"/>
                                    </Storyboard>
                                </VisualState>

        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled"/>
    </Style>
    <Style x:Key="RadMenuSeparatorItemStyle" TargetType="telerikNavigation:RadMenuSeparatorItem" BasedOn="{StaticResource RadMenuItemBaseStyle}">
        <Setter Property="Template" Value="{StaticResource SeparatorTemplate}"/>
    </Style>

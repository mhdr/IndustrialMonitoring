        <Setter Property="Background" Value="{StaticResource LayoutControl_Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource LayoutControl_OuterBorder}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Auto"/>
        <Setter Property="ScrollViewer.VerticalScrollBarVisibility" Value="Auto"/>
        <Setter Property="telerik:DragDropManager.AllowDrag" Value="True"/>
        <Setter Property="ItemsPanel">
            <Setter.Value>
                <ItemsPanelTemplate>
                    <layoutControl:LayoutControlPanel Orientation="{Binding Orientation,RelativeSource={RelativeSource AncestorType=telerikNavigation:RadLayoutControl}}"/>
                </ItemsPanelTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerikNavigation:RadLayoutControl">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <Grid>
                            <ScrollViewer x:Name="PART_RenderSurface"
                                    Margin="0"
                                    IsTabStop="False"
                                    Background="Transparent"
                                    BorderThickness="0"
                                    HorizontalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}"
                                    VerticalScrollBarVisibility="{TemplateBinding ScrollViewer.VerticalScrollBarVisibility}">
                                <ItemsPresenter/>
                            </ScrollViewer>
                            <Canvas x:Name="PART_EditAdorner"
                                    Visibility="{TemplateBinding IsInEditMode, Converter={StaticResource BooleanToVisibilityConverter}}"
                                    Background="#01FFFFFF"
                                    AllowDrop="True"
                                    ClipToBounds="True"/>
                        </Grid>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ManipulationPaneRadioButtonStyle" TargetType="telerik:RadRadioButton">
        <Setter Property="Padding" Value="3"/>
        <Setter Property="Margin" Value="0 0 8 0"/>
        <Setter Property="Foreground" Value="{StaticResource ControlElement_Normal}"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="HorizontalContentAlignment" Value="Center"/>
        <Setter Property="VerticalContentAlignment" Value="Center"/>
        <Setter Property="telerik:RadToolTipService.ToolTipContentTemplate" Value="{x:Null}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="telerik:RadRadioButton">
                    <Border x:Name="BorderVisual"
                            BorderThickness="{TemplateBinding BorderThickness}"
                            BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{TemplateBinding Background}"
                            CornerRadius="{TemplateBinding CornerRadius}">
                        <Path
                                Margin="{TemplateBinding Padding}"
                                Fill="{TemplateBinding Foreground}"
                                VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                                HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                Data="{Binding Content, RelativeSource={RelativeSource TemplatedParent}}"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{StaticResource ControlOuterBorder_MouseOver}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{StaticResource ControlBackground_MouseOver}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{StaticResource ControlOuterBorder_Pressed}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{StaticResource ControlBackground_Pressed}"/>
                        </Trigger>
                        <Trigger Property="IsChecked" Value="True">
                            <Setter TargetName="BorderVisual" Property="BorderBrush" Value="{StaticResource ControlOuterBorder_Checked}"/>
                            <Setter TargetName="BorderVisual" Property="Background" Value="{StaticResource ControlBackground_Checked}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ManipulationPaneButtonStyle" TargetType="telerik:RadPathButton" BasedOn="{StaticResource RadPathButtonStyle}">
        <Setter Property="ContentMargin" Value="0"/>
        <Setter Property="Padding" Value="4"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
        <Setter Property="CornerRadius" Value="0"/>
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="IsBackgroundVisible" Value="False"/>
        <Setter Property="Focusable" Value="False"/>
        <Setter Property="telerik:RadToolTipService.ToolTipContentTemplate" Value="{x:Null}"/>
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="BorderThickness" Value="0"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    <Style x:Key="LayoutControlManipulationPaneStyle" TargetType="layoutControl:LayoutControlManipulationPane">
        <Setter Property="Background" Value="{StaticResource ToolTip_Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource ToolTip_OuterBorder}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="10 0"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="layoutControl:LayoutControlManipulationPane">
                    <Grid>
                        <Border BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}" Background="{TemplateBinding Background}" Margin="0 5">
                            <Grid Margin="{TemplateBinding Padding}">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                    <ColumnDefinition/>
                                </Grid.ColumnDefinitions>
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*"/>
                                    <RowDefinition Height="*"/>
                                </Grid.RowDefinitions>
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="0 10 0 0">
                                    <telerik:RadRadioButton IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Left, Mode=TwoWay}" Style="{StaticResource ManipulationPaneRadioButtonStyle}" Content="M0,0 L2,0 L2,3 L12,3 L12,9 L2,9 L2,12 L0,12 L0,9 L0,3 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Left}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Center, Mode=TwoWay}" Style="{StaticResource ManipulationPaneRadioButtonStyle}" Content="M5,0 L7,0 L7,3 L12,3 L12,9 L7,9 L7,12 L5,12 L5,9 L0,9 L0,3 L5,3 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Center}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Right, Mode=TwoWay}" Style="{StaticResource ManipulationPaneRadioButtonStyle}" Content="M10,0 L12,0 L12,3 L12,9 L12,12 L10,12 L10,9 L0,9 L0,3 L10,3 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Right}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                            IsChecked="{Binding ContextItem.HorizontalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Stretch, Mode=TwoWay}"
                                            Margin="0"
                                            Style="{StaticResource ManipulationPaneRadioButtonStyle}"
                                            Content="M0,0 L2,0 L2,3 L10,3 L10,0 L12,0 L12,3 L12,9 L12,12 L10,12 L10,9 L2,9 L2,12 L0,12 L0,9 L0,3 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_HorizontalAlignment_Stretch}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                </StackPanel>
                                <StackPanel Grid.Row="1" Orientation="Horizontal" VerticalAlignment="Center" Margin="0 8 0 10">
                                    <telerik:RadRadioButton IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Top, Mode=TwoWay}" Style="{StaticResource ManipulationPaneRadioButtonStyle}" Content="M0,0 L3,0 L9,0 L12,0 L12,2 L9,2 L9,12 L3,12 L3,2 L0,2 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Top}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Center, Mode=TwoWay}" Style="{StaticResource ManipulationPaneRadioButtonStyle}" Content="M3,0 L9,0 L9,5 L12,5 L12,7 L9,7 L9,12 L3,12 L3,7 L0,7 L0,5 L3,5 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Center}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Bottom, Mode=TwoWay}" Style="{StaticResource ManipulationPaneRadioButtonStyle}" Content="M3,0 L9,0 L9,10 L12,10 L12,12 L9,12 L3,12 L0,12 L0,10 L3,10 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Bottom}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                    <telerik:RadRadioButton
                                            IsChecked="{Binding ContextItem.VerticalAlignment, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource EnumToBooleanConverter}, ConverterParameter=Stretch, Mode=TwoWay}"
                                            Margin="0"
                                            Style="{StaticResource ManipulationPaneRadioButtonStyle}"
                                            Content="M0,0 L3,0 L9,0 L12,0 L12,2 L9,2 L9,10 L12,10 L12,12 L9,12 L3,12 L0,12 L0,10 L3,10 L3,2 L0,2 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_VerticalAlignment_Stretch}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadRadioButton>
                                </StackPanel>
                                <Border x:Name="SeparatorBorder"
                                        Grid.Column="1"
                                        Grid.RowSpan="2"
                                        Width="1"
                                        VerticalAlignment="Stretch"
                                        BorderBrush="{TemplateBinding BorderBrush}"
                                        BorderThickness="1"
                                        Margin="10 0"/>
                                <StackPanel Grid.Column="2" Grid.RowSpan="2">
                                    <telerik:RadPathButton
                                            Margin="0 10 0 0"
                                            Style="{StaticResource ManipulationPaneButtonStyle}"
                                            Command="{x:Static layoutControl:LayoutControlCommands.SelectParentItem}"
                                            PathGeometry="M0,0 L12,0 12,12 0,12 z M1,3 L11,3 11,11 1,11 z">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=LayoutControl_SelectParent}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadPathButton>
                                    <telerik:RadPathButton
                                            Style="{StaticResource ManipulationPaneButtonStyle}"
                                            Command="{x:Static layoutControl:LayoutControlCommands.DeleteSelectedItem}"
                                            PathGeometry="M0,1L3,1L3,0L9,0L9,1L12,1L12,2L0,2zM1,3L2,3L2,10L4,10L4,3L5,3L5,10L7,10L7,3L8,3L8,10L10,10L10,3L11,3L11,12L1,12z"
                                            Margin="0 8 0 10">
                                        <telerik:RadToolTipService.ToolTipContent>
                                            <telerik:RadToolTipContentView>
                                                <TextBlock Text="{telerik:LocalizableResource Key=Delete}"/>
                                            </telerik:RadToolTipContentView>
                                        </telerik:RadToolTipService.ToolTipContent>
                                    </telerik:RadPathButton>
                                </StackPanel>
                            </Grid>
                        </Border>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="telerikNavigation:RadLayoutControl" BasedOn="{StaticResource RadLayoutControlStyle}"/>
    <Style TargetType="telerikNavigation:LayoutControlGroup" BasedOn="{StaticResource LayoutControlGroupStyle}"/>
    <Style TargetType="telerikNavigation:LayoutControlExpanderGroup" BasedOn="{StaticResource LayoutControlExpanderGroupStyle}"/>
    <Style TargetType="telerikNavigation:LayoutControlTabGroup" BasedOn="{StaticResource LayoutControlTabGroupStyle}"/>
    <Style TargetType="telerikNavigation:LayoutControlTabGroupItem" BasedOn="{StaticResource LayoutControlTabGroupItemContentStyle}"/>
    <Style TargetType="layoutControl:LayoutControlDropIndicator" BasedOn="{StaticResource LayoutControlDropIndicatorStyle}"/>
    <Style TargetType="layoutControl:LayoutControlManipulationPane" BasedOn="{StaticResource LayoutControlManipulationPaneStyle}"/>
    <Style TargetType="layoutControl:LayoutControlSelectionIndicator" BasedOn="{StaticResource LayoutControlSelectionIndicatorStyle}"/>
    <Style TargetType="layoutControl:LayoutControlMouseOverIndicator" BasedOn="{StaticResource LayoutControlMouseOverIndicatorStyle}"/>
    <Style TargetType="layoutControl:LayoutControlDragVisual" BasedOn="{StaticResource LayoutControlDragVisualStyle}"/>
    <DataTemplate x:Key="FooterTemplate">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="auto"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <telerik:RadButton x:Name="PART_Help"
                    Content="{Binding HelpButtonContent, Mode=TwoWay}"
                    MinHeight="22"
                    Margin="10,0,0,0"
                    MinWidth="70"
                    HorizontalAlignment="Left"
                    VerticalAlignment="Center"
                    Visibility="{Binding NavigationButtonsVisibility.HelpButtonVisibility}"
                    Command="{x:Static wizard:RadWizardCommands.Help}"/>
            <StackPanel Grid.Column="1" VerticalAlignment="Center" Orientation="Horizontal" HorizontalAlignment="Right">
                <telerik:RadButton x:Name="PART_Previous"
                        Content="{Binding PreviousButtonContent, Mode=TwoWay}"
                        MinHeight="22"
                        MinWidth="70"
                        VerticalAlignment="Center"
                        Visibility="{Binding NavigationButtonsVisibility.PreviousButtonVisibility}"
                        Command="{x:Static wizard:RadWizardCommands.MoveCurrentToPrevious}"/>
                <telerik:RadButton x:Name="PART_Next"
                        Content="{Binding NextButtonContent, Mode=TwoWay}"
                        Margin="10,0,0,0"
                        MinHeight="22"
                        VerticalAlignment="Center"
                        Visibility="{Binding NavigationButtonsVisibility.NextButtonVisibility}"
                        MinWidth="70"
                        Command="{x:Static wizard:RadWizardCommands.MoveCurrentToNext}"/>
                <telerik:RadButton x:Name="PART_Finish"
                        Content="{Binding FinishButtonContent, Mode=TwoWay}"
                        Margin="10,0,0,0"
                        MinHeight="22"
                        MinWidth="70"
                        VerticalAlignment="Center"
                        Visibility="{Binding NavigationButtonsVisibility.FinishButtonVisibility}"
                        Command="{x:Static wizard:RadWizardCommands.Finish}"/>
                <telerik:RadButton x:Name="PART_Cancel"
                        Content="{Binding CancelButtonContent, Mode=TwoWay}"
                        Margin="10,0,0,0"
                        VerticalAlignment="Center"
                        MinHeight="22"
                        MinWidth="70"
                        Visibility="{Binding NavigationButtonsVisibility.CancelButtonVisibility}"
                        Command="{x:Static wizard:RadWizardCommands.Cancel}"/>
            </StackPanel>
        </Grid>
    </DataTemplate>
    <ControlTemplate x:Key="RadWizardTemplate" TargetType="telerikNavigation:RadWizard">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <ContentPresenter x:Name="PART_SelectedPagePresenter" Content="{Binding SelectedPage, RelativeSource={RelativeSource TemplatedParent}}">
                    <ContentPresenter.Visibility>
                        <Binding RelativeSource="{RelativeSource TemplatedParent}" Path="IsContentPreserved" Converter="{StaticResource InvertedBooleanToVisibilityConverter}"/>
                    </ContentPresenter.Visibility>
                </ContentPresenter>
                <Grid x:Name="ContentElementsPanel" Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsContentPreserved, Converter={StaticResource BooleanToVisibilityConverter}}"/>
            </Grid>
        </Border>
    </ControlTemplate>
    <ControlTemplate x:Key="WizardPageTemplate" TargetType="telerikNavigation:WizardPage">
        <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
            <Grid>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="Auto"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>
                <Border x:Name="PART_SideHeaderBorder"
                        Grid.RowSpan="2"
                        BorderThickness="{Binding SideHeaderBorderWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Right}"
                        Width="{Binding SideHeaderWidth, RelativeSource={RelativeSource TemplatedParent}}"
                        Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsSideHeaderVisible, Converter={StaticResource BooleanToVisibilityConverter}}"
                        BorderBrush="{StaticResource Wizard_BorderBrush}">
                    <ContentPresenter x:Name="PART_SideHeaderPresenter"
                            Margin="{TemplateBinding Padding}"
                            Content="{TemplateBinding SideHeader}"
                            TextBlock.Foreground="{TemplateBinding Foreground}"
                            ContentTemplate="{TemplateBinding SideHeaderTemplate}"/>
                </Border>
                <Border x:Name="PART_HeaderBorder"
                        Grid.Column="1"
                        Grid.ColumnSpan="1"
                        BorderThickness="{Binding HeaderBorderWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Bottom}"
                        Height="{Binding HeaderHeight, RelativeSource={RelativeSource TemplatedParent}}"
                        BorderBrush="{StaticResource Wizard_BorderBrush}">
                    <Grid Margin="{TemplateBinding Padding}">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                        </Grid.RowDefinitions>
                        <TextBlock x:Name="PART_PageTitle"
                                Text="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=Title}"
                                TextWrapping="Wrap"
                                FontFamily="Segoe UI"
                                FontSize="16"
                                FontWeight="Bold"
                                Foreground="{TemplateBinding Foreground}"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsTitleVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                        <ContentPresenter x:Name="PART_HeaderPresenter"
                                Grid.Row="1"
                                Content="{TemplateBinding Header}"
                                TextBlock.Foreground="{TemplateBinding Foreground}"
                                ContentTemplate="{TemplateBinding HeaderTemplate}"
                                Visibility="{Binding RelativeSource={RelativeSource TemplatedParent}, Path=IsHeaderVisible, Converter={StaticResource BooleanToVisibilityConverter}}"/>
                    </Grid>
                </Border>
                <ContentPresenter x:Name="PART_PagePresenter"
                        Content="{TemplateBinding Content}"
                        ContentTemplate="{TemplateBinding ContentTemplate}"
                        Grid.Row="1"
                        Grid.Column="1"
                        Margin="{TemplateBinding Padding}"/>
                <Border x:Name="PART_FooterBorder"
                        Grid.Row="2"
                        Grid.ColumnSpan="2"
                        BorderThickness="{Binding FooterBorderWidth, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DoubleToThicknessConverter}, ConverterParameter=Top}"
                        Background="{StaticResource RadWizard_NavigationPanelBackground}"
                        MinHeight="36"
                        BorderBrush="{StaticResource Wizard_BorderBrush}">
                    <ContentControl x:Name="PART_FooterContentControl"
                            Margin="{TemplateBinding Padding}"
                            ContentTemplate="{TemplateBinding FooterTemplate}"
                            VerticalContentAlignment="Center"
                            HorizontalContentAlignment="Stretch"/>
                </Border>
            </Grid>
        </Border>
    </ControlTemplate>
    <Style x:Key="RadWizardStyle" TargetType="telerikNavigation:RadWizard">
        <Setter Property="Template" Value="{StaticResource RadWizardTemplate}"/>
        <Setter Property="Background" Value="{StaticResource Wizard_Background}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Wizard_BorderBrush}"/>
        <Setter Property="Foreground" Value="{StaticResource Wizard_Foreground}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
    </Style>
    <Style x:Key="WizardPageStyle" TargetType="telerikNavigation:WizardPage">
        <Setter Property="Template" Value="{StaticResource WizardPageTemplate}"/>
        <Setter Property="FooterTemplate" Value="{StaticResource FooterTemplate}"/>
        <Setter Property="BorderBrush" Value="{StaticResource Wizard_BorderBrush}"/>
        <Setter Property="PreviousButtonContent" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Wizard_Previous}"/>
        <Setter Property="NextButtonContent" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Wizard_Next}"/>
        <Setter Property="CancelButtonContent" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Wizard_Cancel}"/>
        <Setter Property="FinishButtonContent" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Wizard_Finish}"/>
        <Setter Property="HelpButtonContent" Value="{Binding Manager.Culture, Source={StaticResource localizationManagerProvider}, Converter={StaticResource cultureToStringConverter}, ConverterParameter=Wizard_Help}"/>
        <Setter Property="Padding" Value="7,6"/>
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="FocusVisualStyle" Value="{x:Null}"/>
        <Setter Property="UseLayoutRounding" Value="True"/>
    </Style>
    <Style TargetType="telerikNavigation:RadWizard" BasedOn="{StaticResource RadWizardStyle}"/>
    <Style TargetType="telerikNavigation:WizardPage" BasedOn="{StaticResource WizardPageStyle}"/>
    <Style TargetType="telerikNavigation:CompletionWizardPage" BasedOn="{StaticResource WizardPageStyle}"/>
    <Style TargetType="telerikNavigation:WelcomeWizardPage" BasedOn="{StaticResource WizardPageStyle}"/>
</ResourceDictionary>

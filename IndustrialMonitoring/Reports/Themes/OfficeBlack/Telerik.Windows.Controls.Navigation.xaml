                        <Storyboard>
                            <DoubleAnimation To="1" Duration="0" Storyboard.TargetName="HighlightVisual" Storyboard.TargetProperty="Opacity"/>
                        </Storyboard>
                    </VisualState>
                    <VisualState x:Name="Disabled">
